{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 35
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/index.ts",
      "headers": {
        "x-vercel-max-duration": "35"
      }
    }
  ]
}
