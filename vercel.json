{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/index.ts"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    },
    "api/*.ts": {
      "maxDuration": 15
    }
  }
}
